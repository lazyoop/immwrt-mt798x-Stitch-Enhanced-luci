{
	"luci-app-frpc": {
		"description": "Grant UCI access for luci-app-frpc",
		"read": {
			"cgi-io": [ "exec" ],
			"file": {
				"/etc/passwd": ["read"],
				"/etc/group": ["read"],
				"/etc/init.d/frpc": [ "exec" ],
				"/usr/bin/frpc": [ "exec" ],
				"/usr/libexec/frpc-call tail": [ "exec" ],
				"/usr/libexec/frpc-call clear_log": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": [ "frpc" ]
		},
		"write": {
			"uci": [ "frpc" ]
		}
	}
}

